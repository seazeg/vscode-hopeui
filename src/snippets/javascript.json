{
    "hopeUI.input.文本框": {
        "prefix": "hope.input",
        "body": [
            "var myinput = hope.input({",
            "    ele: '${1:.ele}',",
            "    options:{",
            "        extendContent:${2:''},",
            "        extendContentLocation:'${3:right}'",
            "    },",
            "    on: {",
            "        blur: function(e) {${4}}",
            "        focus: function(e) {${5}},",
            "        input: function(e) {${6}},",
            "    },",
            "});"
        ],
        "description": "hopeUI文本框组件"
    },
    "hopeUI.suggest.联想文本框": {
        "prefix": "hope.suggest",
        "body": [
            "hope.suggest({",
            "    ele: '${1:.ele}',",
            "    params: {",
            "        url: ${2:''},",
            "        dataType: '${3:json}',",
            "        type: '${4:get}',",
            "        searchField: '${5:keyword}'",
            "    },",
            "    reader: function (res) {",
            "        var list = res.data;",
            "        var tmp = [];",
            "        for (var i = 0; i < list.length; i++) {",
            "            tmp.push(list[i].goodsName)",
            "        }",
            "        return tmp",
            "    },",
            "    on:{",
            "        input:function (e) {${6}}",
            "    }",
            "})"
        ],
        "description": "hopeUI联想文本框组件"
    },
    "hopeUI.textarea.多行文本框": {
        "prefix": "hope.textarea",
        "body": [
            "var mytextarea = hope.textarea({",
            "    ele: '${1:.ele}',",
            "    options:{",
            "        remainingWords:${2:true}",
            "    },",
            "    on: {",
            "        blur: function(e) {${3}},",
            "        focus: function(e) {${4}},",
            "        input: function(e) {${5}},",
            "    },",
            "});"
        ],
        "description": "hopeUI多行文本框组件"
    },
    "hopeUI.carousel.轮播": {
        "prefix": "hope.carousel",
        "body": [
            "var mycarousel = hope.carousel({",
            "    ele: '${1:.ele}',",
            "    options: {",
            "        pagination: '${2:.banner .hope-pagination}',",
            "        loop: ${3:true}",
            "    },",
            "    on:{",
            "        onFirstInit: function (e) {${4}},",
            "        onSlideChangeStart: function (e) {${5}}",
            "    }",
            "})"
        ],
        "description": "hopeUI轮播组件"
    },
    "hopeUI.checkbox.复选框": {
        "prefix": "hope.checkbox",
        "body": [
            "var mycheckbox = hope.checkbox({",
            "    ele: '${1:.ele}',",
            "    on: {",
            "        change: function(e) {${2}},",
            "    },",
            "});"
        ],
        "description": "hopeUI复选框组件"
    },
    "hopeUI.radio.单选框": {
        "prefix": "hope.radio",
        "body": [
            "var myradio = hope.radio({",
            "    ele: '${1:.ele}',",
            "    on: {",
            "        change: function(e) {${2}},",
            "    },",
            "});"
        ],
        "description": "hopeUI单选框组件"
    },
    "hopeUI.radio.日期时间选择器": {
        "prefix": "hope.datepicker",
        "body": [
            "hope.datepicker({",
            "    ele: ${1:#ele}',",
            "    options:{",
            "        format: '${2:yyyy-MM-dd HH:mm:ss}',",
            "        type: '${3:datetime}'//date：日期，datetime:日期+时间",
            "    },",
            "    on:{",
            "        init: function (e){${4}},",
            "        change: function (e){${5}},",
            "        clear: function (e){${6}}",
            "    }",
            "})"
        ],
        "description": "hopeUI日期时间选择器组件"
    },
    "hopeUI.form.表单": {
        "prefix": "hope.form",
        "body": [
            "var myform = hope.form({",
            "    ele: '${1:#ele}',",
            "    on: {",
            "        submit: function(e) {${2}},",
            "        init:function (e) {${3}}",
            "    },",
            "    controls: {",
            "        selector: {",
            "            on: {",
            "                change: function(e) {${4}},",
            "                toggle: function(e) {${5}},",
            "            },",
            "        },",
            "        checkbox: {",
            "            on: {",
            "                change: function(e) {${6}},",
            "            },",
            "        },",
            "        radio: {",
            "            on: {",
            "                change: function(e) {${7}},",
            "            },",
            "        },",
            "        input: {",
            "            on: {",
            "                blur: function(e) {${8}},",
            "                focus: function(e) {${9}},",
            "                input: function(e) {${10}}",
            "            },",
            "        },",
            "        textarea: {",
            "            on: {",
            "                blur: function(e) {${11}},",
            "                focus: function(e) {${12}},",
            "                input: function(e) {${13}}",
            "            },",
            "        },",
            "    },",
            "    verify: {",
            "        //[label-attr-name]:fn",
            "        ${14:labelAttrName}: function(value) {",
            "            if (value.length <= 0) {",
            "                return '请选择一个选项';",
            "            }",
            "        }",
            "    },",
            "});"
        ],
        "description": "hopeUI表单组件"
    },
    "hopeUI.layer.弹窗": {
        "prefix": "hope.layer",
        "body": [
            "var mylayer = hope.layer({",
            "    options: {",
            "        title: '${1:标题}',",
            "        content: '${2:<div style='padding:20px;text-align: center;'>你好，你已经吃过了</div>}',",
            "        defaultBtn: {",
            "            ok: '${3:好的}',",
            "            cancel: '${4:拜拜}',",
            "        },",
            "    },",
            "    on: {",
            "        confirm: function(e) {${5}},",
            "        open: function(e) {${6}},",
            "        close: function(e) {${7}},",
            "        init:function(e) {${8}}",
            "    },",
            "});"
        ],
        "description": "hopeUI弹窗组件"
    },
    "hopeUI.lightbox.图片灯箱": {
        "prefix": "hope.lightbox",
        "body": [
            "hope.lightbox({",
            "    ele: '${1:#ele}',",
            "    options: {",
            "        width: '${2:70%}',",
            "        type: '${3:pic}', //pic,iframe",
            "        maskColor: '${4:rgba(73,74,95,0.8)}',",
            "        animation: '${5:no}',",
            "        prevIcon: '${6:hopeui-icon hopeui-icon-left}',",
            "        nextIcon: '${7:hopeui-icon hopeui-icon-right}',",
            "        closeIcon: '${8:hopeui-icon hopeui-icon-close}'",
            "    },",
            "    on: {",
            "        prev:function (e) {${9}},",
            "        next:function (e) {${10}},",
            "        open:function (e) {${11}},",
            "        close:function (e) {${12}},",
            "        init:function (e) {${13}}",
            "    },",
            "});"
        ],
        "description": "hopeUI图片灯箱组件"
    },
    "hopeUI.loadmore.流式加载": {
        "prefix": "hope.loadmore",
        "body": [
            "var loadmore = hope.loadmore({",
            "    ele: '${1:#ele}',",
            "    options: {",
            "        loadBtnName: '${2:加载更多}',",
            "        autoLoad: ${3:true},",
            "        offset: ${4:30}",
            "    },",
            "    params: {",
            "        url: ${5:''},",
            "        dataType: '${6:json}',",
            "        type: '${7:get}',",
            "        data: {",
            "            pageNo: ${8:1},",
            "            pageSize: ${9:20},",
            "        },",
            "    },",
            "    reader: function (res) {",
            "        //回调示例           ",
            "        var data = res.data;",
            "        var template = '';",
            "        for (var i = 0; i < data.length; i++) {",
            "            template += '<p>' + data[i].goodsName + '|' + data[i].goodsStar + '</p>'",
            "        }",
            "        $('#loadlist').append(template);",
            "        return {",
            "            pageNo: res.pageNo,",
            "            pageSize: res.pageSize,",
            "            totalNumber: res.totalNumber",
            "        }",
            "    },",
            "    on: {",
            "        loaded: function (e) {${10}}",
            "    }",
            "})"
        ],
        "description": "hopeUI流式加载组件"
    },
    "hopeUI.numup.数字自增效果": {
        "prefix": "hope.numup",
        "body": [
            "hope.numup({",
            "    ele: '${1:.ele}',",
            "    options: {  ",
            "        //如果标签上有相应hope-numup-*属性可不写",
            "        num: ${2:52000}",
            "        time: ${3:2000},",
            "        speed: ${4:50},",
            "    },",
            "    on: {",
            "        end: function (e) {${5}}",
            "    }",
            "})"
        ],
        "description": "hopeUI数字自增效果组件"
    },
    "hopeUI.picture.图片响应式加载": {
        "prefix": "hope.picture",
        "body": [
            "hope.picture({",
            "    ele: '${1:.ele}',",
            "    options: {",
            "        breakpoint: {",
            "            //每个端的最min断点，默认xl.1200,md.750",
            "            xl: 1200,",
            "            lg: 960,",
            "            md: 768,",
            "            sm: 575,",
            "            xs: 0",
            "        }",
            "    },",
            "    on: {",
            "        resize: function (e) {${2}}",
            "    }",
            "})"
        ],
        "description": "hopeUI图片响应式加载组件"
    },
    "hopeUI.omit.文字响应式截断": {
        "prefix": "hope.omit",
        "body": [
            "hope.omit({",
            "    ele: '${1:.ele}',",
            "    options:{",
            "        breakpoint: {",
            "            'xl': {",
            "                point: 1200,",
            "                row: 4",
            "            },",
            "            'xs': {",
            "                point: 360,",
            "                row: 1",
            "            }",
            "        }",
            "    },",
            "    on: {",
            "        resize: function (e) {${2}}",
            "    }",
            "})"
        ],
        "description": "hopeUI文字响应式截断组件"
    },
    "hopeUI.omit.自定义滚动条": {
        "prefix": "hope.scrollbar",
        "body": [
            "hope.scrollbar({",
            "    ele: '${1:#ele}',",
            "    options:{",
            "        height: ${2:200},",
            "        autoHideBar: ${3:true}",
            "    },",
            "    on: {",
            "        init: function (e) {${4}},",
            "        scroll: function (e) {${5}}",
            "    }",
            "})"
        ],
        "description": "hopeUI自定义滚动条组件"
    },
    "hopeUI.tab.选项卡": {
        "prefix": "hope.tab",
        "body": [
            "hope.tab({",
            "    ele: '${1:#ele}',",
            "    options: {",
            "        slideSwitch: ${2:false},",
            "    },",
            "    on: {",
            "        change: function(e) {${3}},",
            "        init: function(e) {${4}},",
            "    }",
            "});"
        ],
        "description": "hopeUI选项卡组件"
    },
    "hopeUI.zoom.图像放大镜": {
        "prefix": "hope.zoom",
        "body": [
            "hope.zoom({",
            "    ele: '${1:#ele}',",
            "    options: {",
            "        largeWrapper: '${2:preview}',",
            "        mode: '${3:outside}',",
            "        zoom: ${4:2},",
            "        zoomable: ${5:true}",
            "    },",
            "    on:{",
            "        enter: function (e) {${6}},",
            "        move: function (e) {${7}},",
            "        leave: function (e) {${8}},",
            "        scale: function (e) {${9}},",
            "    }",
            "});"
        ],
        "description": "hopeUI图像放大镜组件"
    },
    "hopeUI.upload.文件上传": {
        "prefix": "hope.upload",
        "body": [
            "hope.upload({",
            "    ele: '${1:#ele}',",
            "    options: {",
            "        url: '${2}',",
            "        name: '${3}',",
            "        type: ${4:['jpg','jpeg','png']},",
            "        size: ${5:5 * 1024 * 1024}",
            "        multiple: ${6:true}",            
            "    },",
            "    on:{",
            "        validate: function (e) {${7}},",
            "        uploading: function (e) {${8}},",
            "        complete: function (e) {${9}},",
            "        error: function (e) {${10}},",
            "    }",
            "});"
        ],
        "description": "hopeUI文件上传组件"
    },
    "hopeUI.pager.分页器": {
        "prefix": "hope.pager",
        "body": [
            "var mypager = hope.pager({",
            "    ele: '${1:#ele}',",
            "    options: {",
            "        omit: ${2:5},",
            "        prevName: '${3:prev}',",
            "        nextName: '${4:next}',",
            "        hideNum: ${5:false},",
            "        pageMapping: '${6:pageNo}', //当前页码字段的映射，默认pageNo",
            "        extend: ${7:true},",
            "    },",
            "    params: {",
            "        url: ${8:''},",
            "        dataType: '${9:json}',",
            "        type: '${10:get}',",
            "        data: {",
            "            pageNo: ${11:1},",
            "            pageSize: ${12:20},",
            "        },",
            "    },",
            "    reader: function(res) {",
            "        //回调示例           ",
            "        var data = res.data;",
            "        var template = '';",
            "        for (var i = 0; i < data.length; i++) {",
            "            template +=",
            "                '<p>' +",
            "                data[i].goodsName +",
            "                '|' +",
            "                data[i].goodsStar +",
            "                '</p>';",
            "        }",
            "        $('#pagelist2').html(template);",
            "        return {",
            "            pageNo: res.pageNo,",
            "            pageSize: res.pageSize,",
            "            totalNumber: res.totalNumber,",
            "        };",
            "    },",
            "    on: {",
            "        jumpOver: function(e) {${13}},",
            "    },",
            "});"
        ],
        "description": "hopeUI分页器组件"
    }
}